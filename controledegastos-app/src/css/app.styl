// app global css
  @import '~variables';

  body {
    font-size: 14px;
  }

  .body {
    background-color: linear-gradient(#ffffff, #f2f2f2);
    font-family: 'Roboto', sans-serif;
    font-size: 14px;
  }

  .rodape {
    min-height: 30px
  }

  .q-card {
    margin: 0 8px 0px 8px !important
  }

  .layout-page-container {
    padding-top: 47px !important
  }

  .fullscreen {
    z-index: 2000;
  }

  .q-if-label {
    color: rgba(0, 0, 0, .7) !important
  }

  .disabled,

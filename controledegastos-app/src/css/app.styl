  }

  .primary-background {
    color: #fff;
    background-color: $toolbar-background;
  }

  .degrade-background {
    background: linear-gradient(to right, #f9b357 , #bf5d60);
    text-align: center;
  }
  
  /************ CAMINHO DE MIGALHA ************/

ul.breadcrumb{list-style:none;display:-ms-inline-flexbox;display:inline-flex;-ms-flex-wrap:nowrap;flex-wrap:nowrap;margin:11px 0 0;padding:0}
ul.breadcrumb i{font-size:1.7em}
ul.breadcrumb li{float:left;margin-bottom:11px}
ul.breadcrumb li a{color:#fff;display:block;background:#4f6984;text-decoration:none;position:relative;height:34px;line-height:34px;padding:0 10px 0 5px;text-align:center;margin-right:23px}
ul.breadcrumb li:nth-child(2n) a{background:#5a7897}
ul.breadcrumb li:nth-child(2n) a:before{border-color:#5a7897;border-left-color:transparent}
ul.breadcrumb li:nth-child(2n) a:after{border-left-color:#5a7897}
ul.breadcrumb li:first-child a{padding-left:15px;border-radius:2px 0 0 2px}
ul.breadcrumb li:first-child a:before{border:0}
ul.breadcrumb li:last-child a{padding-right:15px;border-radius:0 2px 2px 0}
ul.breadcrumb li:last-child a:after{border:0}
ul.breadcrumb li a:after,ul.breadcrumb li a:before{content:"";position:absolute;top:0;bottom:0;border:0 solid #4f6984;border-width:17px 10px}
ul.breadcrumb li a:before{left:-15px;border-left-color:transparent}
ul.breadcrumb li a:after{left:100%;border-color:transparent;border-left-color:#4f6984}
ul.breadcrumb li a:active{background:#208d83}
ul.breadcrumb li a:active:before{border-color:#208d83;border-left-color:transparent}
ul.breadcrumb li a:active:after{border-left-color:#208d83}


  .fixo-topo {
    z-index: 1000;
    position: -webkit-sticky; /* Safari */
    position: sticky;
    top: 60px;
  }

  .gvn-breadcrumb {
    padding: 0px 0px 6px 0px  !important;
    box-shadow: 0 0 0 !important;
    background: #fff !important;
    cursor: pointer;
  }

  .gvn-breadcrumb .breadcrumb {
    background-color: #fff;
    margin-top: 8px !important;
    width: 100%;
  }

  .gvn-breadcrumb ul li {
    margin-bottom: 0 !important;
    margin-right: 0 !important;
    width: 75px;
  }

  .gvn-breadcrumb ul li a {
    background-color: #bf5d60 !important;
    text-align: left;
  }

  .gvn-breadcrumb ul li a span {
    display: none
  }

  .gvn-breadcrumb ul li .q-icon {
    margin-right: 5px !important
  }

  .gvn-breadcrumb ul li a:after {
    border-left-color: #bf5d60 !important
  }

  .gvn-breadcrumb ul li a::before {
    border-color:#bf5d60 !important;
    border-left-color: transparent !important;
  }

  /****** EFEITO HOVER ******/

  .gvn-breadcrumb ul li:hover {
    width: 100% !important
  }

  .gvn-breadcrumb ul li:hover a {
    background-color: #f9b357 !important
  }
  
  .gvn-breadcrumb ul li:hover a span {
    display: inline !important
  }

  .gvn-breadcrumb ul li:hover a::after {
    border-left-color: #f9b357 !important;
  }

  .gvn-breadcrumb ul li:hover a::before {
    border-color: #f9b357 !important;
    border-left-color: transparent !important;
  }

  /****** EFEITOS DA ÚLTIMA PÁGINA ******/

  .gvn-breadcrumb ul li:last-child {
    width: 100% !important
  }

  .gvn-breadcrumb ul li:last-child a {
    cursor: default !important;
    margin-right: 0 !important;
    border-radius: 0 !important;
    background-color: #bf5d60 !important
  }

  .gvn-breadcrumb ul li:last-child a span {
    display: inline !important
  }

  .gvn-breadcrumb ul li:last-child a label {
    cursor: default !important
  }

  .gvn-breadcrumb ul li:last-child:hover a {
    background-color: #bf5d60 !important
  }

  .gvn-breadcrumb ul li:last-child:hover a::before {
    border-color:#bf5d60 !important;
    border-left-color: transparent !important;
  }


